{
	"compatibility_date": "2025-10-08",
	"main": "src/index.ts",
	"name": "drive",
	"assets": {
		"directory": "node_modules/r2-explorer/dashboard",
		"binding": "ASSETS",
		"html_handling": "auto-trailing-slash",
		"not_found_handling": "single-page-application"
	},
	"r2_buckets": [
		{
			"binding": "bucket",
			"bucket_name": "drive",
			"preview_bucket_name": "drive"
		}
	],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true
}
